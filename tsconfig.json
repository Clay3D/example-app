{
    "compilerOptions": {
        "module": "ES2020",
        "moduleResolution": "node",
        "baseUrl": "./resources/js/",
        "paths": {
            "@/*": ["*"]
        },
        "target": "es5",
        "outDir": "./public",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "alwaysStrict": true,
        "skipLibCheck": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "pretty": true
    }
}
